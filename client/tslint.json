{
	"extends": "tslint-config-airbnb",
		"rules": {
			"indent": [true, "tabs", 2],
			"ter-indent": [true, "tab"],
			"curly": [true, "as-needed"],
			"whitespace": [true, "check-operator"],
			"object-curly-spacing": [true, "never"],
			"import-name": false,
			"variable-name": false,
			"max-line-length": [true, 140],
			"one-variable-per-declaration": false,
		}
	}